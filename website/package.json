    "polished": "^4.3.1",
    "prop-types": "^15.8.1",
    "rc-slider": "^9.7.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-media": "^1.10.0",
    "react-router-dom": "^7.1.1",
    "react-tweet-embed": "^2.0.0",
