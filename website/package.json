    "weathericons": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/d3": "^7.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.6"
  },
  "scripts": {
    "analyze": "vite build && source-map-explorer 'dist/**/*.js' --no-border-checks",
    "lint": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css}' && echo 'Running ESLint...' && eslint src --report-unused-disable-directives --max-warnings 0 && echo 'All files pass linting!'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css}'",
    "start": "vite",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc && vite build",
    "preview": "vite preview",
    "update-deps": "npx npm-check-updates -u --reject rc-slider && npm install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
